<SYNC Start=1715766><P Class=KRCC>&nbsp;
<SYNC Start=1717834><P Class=KRCC>&nbsp;
<SYNC Start=1723441><P Class=KRCC>&nbsp;
<SYNC Start=1725976><P Class=KRCC>&nbsp;
<SYNC Start=1730948><P Class=KRCC>&nbsp;
<SYNC Start=1733150><P Class=KRCC>&nbsp;
<SYNC Start=1735336><P Class=KRCC>&nbsp;
<SYNC Start=1738205><P Class=KRCC>&nbsp;
<SYNC Start=1738207><P Class=KRCC>&nbsp;
<SYNC Start=1740941><P Class=KRCC>&nbsp;
<SYNC Start=1754822><P Class=KRCC>&nbsp;
<SYNC Start=1757291><P Class=KRCC>&nbsp;
<SYNC Start=1760194><P Class=KRCC>
