<SYNC Start=11818><P Class=ENUSCC>
<SYNC Start=17755><P Class=ENUSCC>&nbsp;
<SYNC Start=17757><P Class=ENUSCC>
<SYNC Start=20124><P Class=ENUSCC>&nbsp;
<SYNC Start=20126><P Class=ENUSCC>
<SYNC Start=22860><P Class=ENUSCC>&nbsp;
<SYNC Start=22862><P Class=ENUSCC>
<SYNC Start=26430><P Class=ENUSCC>&nbsp;
<SYNC Start=26432><P Class=ENUSCC>
